/* define HAVE_IN_ADDR_T if your system has a typedef for in_addr_t */
#undef HAVE_IN_ADDR_T

/* define HAVE_U_INT32_T if your system has a typedef for u_int32_t */
#undef HAVE_U_INT32_T

/* define HAVE_TIMESTAMP_IOCTL if your system has an ioctl for getting
	packet timestamps (Linux) */
#undef HAVE_TIMESTAMP_IOCTL

/* define HAVE_SO_TIMESTAMP if your system has an SO_TIMESTAMP for getting
	packet timestamps (FreeBSD) */
#undef HAVE_SO_TIMESTAMP

/* define on systems with sched.h */
#undef HAVE_SCHED_H

/* define on systems with sys/mman.h */
#undef HAVE_SYS_MMAN_H

/* define VHF to enable VHF averaging filter */
#undef VHF

/* define RT_PROCES to enable ASSOLO running as a soft-rt process (FIFO sched) */
#undef RT_PROCESS
